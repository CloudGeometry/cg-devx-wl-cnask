{
  "name": "cnask-api",
  "version": "0.0.1",
  "description": "NestJS Prisma GraphQL Starter Project",
  "author": "igroylova",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "copyfiles -e \"*.ts\" src/*.graphql prisma/schema.prisma prisma/migrations/**/*.* prisma/dbml/*.* dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npx prisma generate",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pre:push:lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "migrate": "prisma migrate dev --preview-feature",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "yarn migrate:up && yarn prisma:generate && yarn seed",
    "dbmigrate": "npx prisma migrate deploy && npx prisma db seed",
    "seed": "ts-node prisma/seed.ts",
    "seed:local": "ts-node prisma/seed.ts",
    "seed:dist": "node dist/prisma/seed.js",
    "seed:docker": "node prisma/seed.js"
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@apollo/client": "^3.7.13",
    "cross-fetch": "^3.1.5",
    "@aws-sdk/client-s3": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.370.0",
    "@casl/ability": "^6.5.0",
    "@devoxa/prisma-relay-cursor-connection": "^2.2.2",
    "@apollo/subgraph": "^2.0.0",
    "@nestjs/apollo": "11.0.5",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "9.3.10",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.10",
    "@nestjs/graphql": "11.0.5",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.3.10",
    "@nestjs/terminus": "^10.0.1",
    "@prisma/client": "4.13.0",
    "@types/nodemailer": "^6.4.8",
    "bcrypt": "^5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "axios": "^1.0.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "1.20.1",
    "graphql-subscriptions": "2.0.0",
    "nestjs-prisma": "0.22.0",
    "node-fetch": "^3.3.1",
    "nodemailer": "^6.9.3",
    "@aws-sdk/client-ses": "~3.509.0",
    "@aws-sdk/credential-providers": "~3.509.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "uuid": "~9.0.1",
    "@willsoto/nestjs-prometheus": "~5.2.1",
    "prom-client": "~14.2.0",
    "copyfiles": "~2.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.4.0",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "^18.15.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "chance": "1.1.11",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.4",
    "prisma": "4.13.0",
    "prisma-dbml-generator": "^0.10.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-morph": "^17.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
